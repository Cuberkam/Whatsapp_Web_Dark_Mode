{
  "name": "WhatsApp Web Dark Mode",
  "version": "2.0.20.0",
  "description": "Dark mode for WhatsApp Web",
  "author": "Erkam Kumru",
  "content_scripts": [
    {
      "matches": [
        "https://web.whatsapp.com/"
      ],
      "css": [
        "style.css"
      ]
    }
  ],
  "browser_action": {
    "default_icon": {
      "128": "icon/whatsapp_dark_mode_128.png"
    }
  },
  "icons": {
    "16": "icon/whatsapp_dark_mode_16.png",
    "32": "icon/whatsapp_dark_mode_32.png",
    "48": "icon/whatsapp_dark_mode_48.png",
    "64": "icon/whatsapp_dark_mode_64.png",
    "128": "icon/whatsapp_dark_mode_128.png"
  },
  "manifest_version": 2
}